{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*\\.(js|css|woff|woff2|ttf|eot|png|jpg|jpeg|gif|ico|json|svg))",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      },
      "dest": "/dist/$1"
    },
    {
      "src": "/",
      "dest": "/index.js"
    }
  ]
}
